<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane focusTraversable="false" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="world.fantasy.controllers.FullMapController">
  <content>
    <AnchorPane fx:id="display">
         <children>
            <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/ground.png" />
               </image>
            </ImageView>
            <fx:include fx:id="move" source="move-input-view.fxml" />
         </children>
      </AnchorPane>
  </content>
</ScrollPane>
